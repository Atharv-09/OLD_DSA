class Node{
    int data;
    Node next;

    Node(int d){
        data = d;
        next = null;
    }
}

class LinkedList{
    Node head;


    void printList(){

        Node temp = head;
        while(temp!=null){
            System.out.print(temp.data+ " " );
        }
        System.out.println();
    }
    

    public static void main(String[] args){
        LinkedList ll = new LinkedList();


        ll.head = new Node(1);
        ll.head.next  =  new Node(2)
        ll.head.next.next = new Node(3);
        ll.head.next.next.next = new Node(4);

        ll.printList();
    }
}